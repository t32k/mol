---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.inc.title }}</title>
    <link>{{ site.url }}</link>
    <atom:link href="{{ site.url }}mol/feed.xml" rel="self" type="application/rss+xml" />
    <description>{{ site.inc.subtitle }}</description>
    <language>ja-jp</language>
    <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

    {% for post in site.posts limit:20 %}
    {% unless post.draft %}
    <item>
      <title>{{ post.title }}</title>
      <link>{{ site.url }}mol{{ post.url }}</link>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
      <author>Koji Ishimoto</author>
      <guid>{{ site.url }}mol{{ post.id }}</guid>
      <description>{{ post.content | xml_escape }}</description>
    </item>
    {% endunless %}
    {% endfor %}

  </channel> 
</rss>